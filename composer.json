    "autoload-dev": {
        "psr-4": {
            "Ruler\\Test\\": "tests/Ruler/Test"
        }
    },
    "authors": [
        {
            "name": "Justin Hileman",
            "email": "justin@justinhileman.info",
            "homepage": "http://justinhileman.com/"
        }
    ]
