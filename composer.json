    "keywords": ["rules", "engine"],
    "homepage": "https://github.com/bobthecow/Ruler",
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.12 | ^9"
    },
    "autoload": {
        "psr-4": {
            "Ruler\\": "src/Ruler"
        }
